{
  "name": "chaos-cms-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Modern CMS Frontend for Chaos World built with Next.js 14, TypeScript, and Tailwind CSS",
  "author": "Chaos World Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next node_modules/.cache",
    "analyze": "ANALYZE=true npm run build",
    "validate-env": "node scripts/validate-env.js"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "next-auth": "^4.24.0",
    "next-themes": "^0.2.1",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "@headlessui/react": "^1.7.0",
    "lucide-react": "^0.300.0",
    "framer-motion": "^10.16.0",
    "clsx": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "tailwind-merge": "^2.0.0",
    "zustand": "^4.4.0",
    "@tanstack/react-query": "^5.0.0",
    "react-hook-form": "^7.47.0",
    "@hookform/resolvers": "^3.3.0",
    "zod": "^3.22.0",
    "axios": "^1.6.0",
    "jose": "^5.0.0",
    "date-fns": "^2.30.0",
    "react-hot-toast": "^2.4.0",
    "react-intersection-observer": "^9.5.0",
    "react-use": "^17.4.0",
    "use-debounce": "^9.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss-animate": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "cms",
    "content-management",
    "nextjs",
    "react",
    "typescript",
    "tailwindcss",
    "frontend",
    "chaos-world"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chaos-world/chaos-cms-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/chaos-world/chaos-cms-frontend/issues"
  },
  "homepage": "https://github.com/chaos-world/chaos-cms-frontend#readme"
}